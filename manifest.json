{
  "manifest_version": 3,
  "name": "Last Used Tab Switcher",
  "version": "1.1",
  "description": "Switch between current tab and last used tab, similar to Firefox behavior",
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "switch-to-last-tab": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "Alt+Tab"
      },
      "description": "Switch to last used tab"
    },
    "duplicate-tab": {
      "suggested_key": {
        "default": "Ctrl+Shift+D",
        "mac": "Alt+Shift+D"
      },
      "description": "Duplicate current tab"
    }
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Last Used Tab Switcher"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
